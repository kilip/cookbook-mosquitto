{
  "path": "/etc/mosquitto",
  "allow_anonymous": "false",
  "password_file": "/etc/mosquitto/passwd",
  "cafile": "/etc/mosquitto/certs/cafile.pem",
  "certfile": "/etc/mosquitto/certs/certfile.pem",
  "keyfile": "/etc/mosquitto/certs/keyfile.pem",
  "require_certificate": "false",
  "tls_version": "tlsv2",
  "users": [
    {
      "username": "foo",
      "password": "bar"
    },
    {
      "username": "hello",
      "password": "world"
    }
  ],
  "listeners": [
    {
      "port": 1883,
      "addr": "127.0.0.1"
    },
    {
      "port": 8883,
      "addr": "0.0.0.0"
    },
    {
      "port": 9001,
      "protocol": "websockets"
    }
  ]
}
